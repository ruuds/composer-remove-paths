{
  "name": "ruuds/composer-remove-paths",
  "description": "Composer plugin for removing files after install or update.",
  "type": "composer-plugin",
  "keywords": [
    "composer-plugin",
    "installer",
    "remove file"
  ],
  "homepage": "https://github.com/ruuds/composer-remove-paths",
  "authors": [
    {
      "name": "Ruud Simons",
      "email": "post@ruudsimons.nl",
      "homepage": "https://www.ruudsimons.nl"
    }
  ],
  "require": {
    "composer-plugin-api": "^1.0",
    "phpunit/phpunit": "^9.3"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev"
  },
  "autoload": {
    "psr-4": {
      "ruuds\\Composer\\": "src/",
      "ruuds\\Composer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "class": "ruuds\\Composer\\Plugin"
  }
}
