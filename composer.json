{
  "name": "ruuds/composer-remove-paths",
  "description": "Composer plugin for removing files after install or update",
  "type": "composer-plugin",
  "keywords": [
    "composer-plugin",
    "installer",
    "remove file"
  ],
  "homepage": "https://github.com/ruuds/composer-remove-paths",
  "authors": [
    {
      "name": "Ruud Simons",
      "email": "ruud@groowup.nl",
      "homepage": "http://www.groowup.nl"
    }
  ],
  "require": {
    "composer-plugin-api": "^1.0"
  },
  "require-dev": {
    "composer/composer": "1.0.*@dev",
    "phpunit/phpunit": "4.4.*"
  },
  "autoload": {
    "psr-4": {
      "ruuds\\Composer\\": "src/"
    }
  },
  "extra": {
    "class": "ruuds\\Composer\\Plugin"
  }
}